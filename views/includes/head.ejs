<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content=<%=head.metaDescription %>>
    <meta name="keywords" content=<%=head.metaKeywords %>>
    <!-- Fav Icon  -->
    <link rel="shortcut icon" href=<%=head.favIcon %>>
    <!-- Site Title  -->
    <title>
        <%= head.title %>
    </title>
    <% head.cssFiles.forEach(function(cssFile){ %>
        <link href=<%=cssFile %> rel="stylesheet">
        <% }); %>
            <% head.jsFiles.forEach(function(jsFile){ %>
                <script src=<%=jsFile %>
                    >
                </script>
                <% }); %>

</head>